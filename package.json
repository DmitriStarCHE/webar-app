{
  "name": "webar-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "WebAR application with admin panel and AR viewer",
  "scripts": {
    "dev": "pnpm --parallel --filter \"./apps/*\" dev",
    "build": "pnpm --filter \"./apps/*\" build",
    "lint": "pnpm --filter \"./apps/*\" lint",
    "type-check": "pnpm --filter \"./apps/*\" type-check",
    "clean": "pnpm --recursive exec rm -rf node_modules dist .turbo",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:migrate": "pnpm --filter database migrate",
    "db:studio": "pnpm --filter database studio",
    "db:generate": "pnpm --filter database generate"
  },
  "keywords": [
    "webAR",
    "augmented-reality",
    "three.js",
    "mindAR",
    "react"
  ],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "pnpm": {
    "overrides": {
      "canvas": "npm:empty-npm-package@1.0.0"
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "prettier": "^3.2.5",
    "turbo": "^1.13.0",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "exa-js": "^2.0.11",
    "openai": "^6.10.0"
  }
}
